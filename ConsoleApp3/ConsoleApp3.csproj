    <Reference Include="PdfSharp.Charting">
      <HintPath>Dlls\PdfSharp.Charting.dll</HintPath>
    </Reference>
    <Reference Include="PdfSharp.Quality">
      <HintPath>Dlls\PdfSharp.Quality.dll</HintPath>
    </Reference>
    <Reference Include="PdfSharp.Snippets">
      <HintPath>Dlls\PdfSharp.Snippets.dll</HintPath>
    </Reference>
    <Reference Include="PdfSharp.System">
      <HintPath>Dlls\PdfSharp.System.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
